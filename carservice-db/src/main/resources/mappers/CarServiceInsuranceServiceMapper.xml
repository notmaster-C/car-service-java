<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.click.carservice.db.mapper.CarServiceInsuranceServiceMapper">

    <resultMap type="org.click.carservice.db.domain.CarServiceInsuranceService" id="CarServiceInsuranceServiceResult">
        <result property="id"    column="id"    />
        <result property="insuranceInfoId"    column="insurance_info_id"    />
        <result property="serviceName"    column="service_name"    />
        <result property="serviceCode"    column="service_code"    />
        <result property="serviceTotal"    column="service_total"    />
        <result property="serviceUsed"    column="service_used"    />
        <result property="addTime"    column="add_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="deleted"    column="deleted"    />
        <result property="tenantId"    column="tenant_id"    />
        <result property="version"    column="version"    />
    </resultMap>

    <resultMap id="CarServiceInsuranceServiceCarServiceInsuranceServiceOrderResult" type="org.click.carservice.db.domain.CarServiceInsuranceService" extends="CarServiceInsuranceServiceResult">
        <collection property="carServiceInsuranceServiceOrderList" notNullColumn="sub_id" javaType="java.util.List" resultMap="CarServiceInsuranceServiceOrderResult" />
    </resultMap>

    <resultMap type="org.click.carservice.db.domain.CarServiceInsuranceServiceOrder" id="CarServiceInsuranceServiceOrderResult">
        <result property="id"    column="sub_id"    />
        <result property="insuranceServiceId"    column="sub_insurance_service_id"    />
        <result property="orderId"    column="sub_order_id"    />
    </resultMap>

    <sql id="selectCarServiceInsuranceServiceVo">
        select id, insurance_info_id, service_name, service_code, service_total, service_used, add_time, update_time, deleted, tenant_id, version from car_service_insurance_service
    </sql>

</mapper>